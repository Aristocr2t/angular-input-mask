{
  "name": "ngx-input-mask",
  "version": "1.0.1",
  "description": "Simple input mask directive for angular",
  "jsnext:main": "bundles/input-mask.esm.js",
  "main": "bundles/input-mask.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "dependencies": {
    "@angular/core": "^5.0.1",
    "@angular/forms": "^5.0.1",
    "rxjs": "^5.5.2"
  },
  "devDependencies": {
    "fs": "0.0.1-security",
    "node-sass": "^4.7.2",
    "rollup": "^0.51.5",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.6.1"
  },
  "peerDependencies": {
    "@angular/core": ">=2.0.0 || >=4.0.0",
    "@angular/forms": ">=2.0.0 || >=4.0.0",
    "rxjs": ">=5.0.0"
  },
  "scripts": {
    "build": "npm run bundle && npm run copy",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc",
    "copy": "node tools/copy-package.js",
    "publish": "cd ./dist && npm publish"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular 2",
    "angular 4",
    "angular 5",
    "input-mask"
  ],
  "author": {
    "name": "Shamil' Shaymardanov",
    "email": "shamil.amirovich@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aristocr2t/angular-input-mask.git"
  },
  "bugs": {
    "url": "https://github.com/Aristocr2t/angular-input-mask/issues"
  },
  "homepage": "https://github.com/Aristocr2t/angular-input-mask#readme"
}
