{
	"name": "ngx-input-mask",
	"version": "2.0.1",
	"description": "Input mask directive for angular",
	"jsnext:main": "ngx-input-mask.esm.js",
	"main": "ngx-input-mask.umd.js",
	"module": "index.js",
	"typings": "index.d.ts",
	"scripts": {
		"prebuild": "npm install && rimraf dist/",
		"build": "rollup -c tools/rollup-config.js && node tools/build.js",
		"prepublishOnly": "npm run build && node tools/prepublish.js",
		"release": "RELEASE_MODE=1 npm run prepublishOnly && npm publish --access public dist/",
		"lint": "eslint \"src/**/*.ts\" -f codeframe"
	},
	"devDependencies": {
		"@evojs/eslint-plugin": "^0.1.0",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-typescript": "^8.1.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.17.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.36.1",
		"rollup-plugin-angular": "^0.5.3",
		"rollup-plugin-typescript2": "^0.29.0",
		"typescript": "^4.1.3"
	},
	"peerDependencies": {
		"@angular/core": ">=9.0.0",
		"@angular/forms": ">=9.0.0",
		"rxjs": ">=6.0.0"
	},
	"keywords": [
		"angular",
		"angular2",
		"ngx-input-mask",
		"input-mask"
	],
	"author": {
		"name": "Shamil Sultanov",
		"email": "shamil.amirovich@gmail.com"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aristocr2t/ngx-input-mask.git"
	},
	"bugs": {
		"url": "https://github.com/aristocr2t/ngx-input-mask/issues"
	},
	"homepage": "https://github.com/aristocr2t/ngx-input-mask#readme"
}
